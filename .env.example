# Server-to-server API key (keep the real value in .env only)
EMBED_API_KEY=change-me

# Embedding model (dimension 384 for all-MiniLM-L6-v2)
EMBED_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Optional names (not used by CockroachDB directly but kept for clarity)
EMBED_COLLECTION=embeddings
VECTOR_INDEX_NAME=vector_index
EMBED_DIM=384

# CockroachDB connection string (Postgres protocol)
# Example format:
# postgresql://USERNAME:PASSWORD@HOST:PORT/defaultdb?sslmode=require
DATABASE_URL=postgresql://user:pass@host:26257/defaultdb?sslmode=require
